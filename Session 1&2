        "cp = 10000\n",
        "# find population\n",
        "r = 10\n",
        "i = 2030\n",
        "for i in range(2030, 2020,-1):\n",
        "    print(i, cp)\n",
        "# in previous 10 year population will decrease\n",
        "    cp = cp - 0.1 * cp\n",
        "\n",
        "# formula\n",
        "# 10 year = P(1+r/100)\n",
        "# 10 year = P(1+10/100)\n",
        "# print(10 year)\n",
        "\n",
        "# 10 % of 9000 population is 999"
      ],
      "metadata": {
        "id": "i_3rQjQuiL3l"
      },
      "execution_count": null,
      "outputs": []
    },
    {

      "source": [
        "correct:"
      ],
      "metadata": {
        "id": "Gjbia1fadFCT"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "x = int(input(\"Enter the number: \"))\n",
        "count = 0  # Initialize the counter outside the loop\n",
        "\n",
        "while x < 8:\n",
        "    print(x)\n",
        "    count += 1  # Increment the counter in each iteration\n",
        "    x += 1  # Add this to ensure the loop eventually exits, otherwise it will be infinite\n",
        "\n",
        "else:\n",
        "    print(\"Limit crossed\")\n",
        "    print(f\"The while loop ran {count} times.\")  # Display the number of iterations"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"

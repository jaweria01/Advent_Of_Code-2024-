        "pp = []\n",
        "# make list\n",
        "# apply for loop\n",
        "# increment of population\n",
        "for i in range(1,11):\n",
        "  r = cp/(1+0.1)**i\n",
        "  pp.append(round(r))\n",
        "print(pp)"

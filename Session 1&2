        "\n",
        "if op == '+' :\n",
        "    print(Fnum + Snum)\n",
        "\n",
        "elif op == '-' :\n",
        "    print(Fnum - Snum)\n",
        "\n",
        "elif op == '*' :\n",
        "    print(Fnum * Snum)\n",
        "\n",
        "elif op == '/':\n",
        "    print(Fnum * Snum)\n",
        "\n",
        "elif op == '//'\n",
        "\n",
        "else:\n",
        "    print(Fnum% Snum)"
